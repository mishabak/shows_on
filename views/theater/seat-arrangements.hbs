<script>
    var seat = document.getElementsByClassName('seat');
    var seatPosition;
    var cd;
    var sd;
    function seatArrangement(div) {
        seat = [].slice.call(seat);
        seatPosition = seat.indexOf(div)
        cd = document.getElementsByClassName('seat')[seatPosition].style.background
        document.getElementsByClassName('seat')[seatPosition].style.background = 'red'
        sd = document.getElementsByClassName('seat')[seatPosition].style.background
        if (cd === sd) {
            document.getElementsByClassName('seat')[seatPosition].style.background = 'rgb(49, 49, 49)'
        }
    }

    function saveSeatArrangement(screenId) {
        seat = [].slice.call(seat);
        var seatArray = new Array();
        var flag = 0
        for (var i = 0; i < seat.length; i++) {
            var d = document.getElementsByClassName('seat')[i].style.background
            if (seat[i].style.background == 'rgb(49, 49, 49) none repeat scroll 0% 0%') {  // for firefox
                flag = 1
            } else if (seat[i].style.background == 'rgb(49, 49, 49)') {  //for crome and avast browser
                flag = 1
            } else {
                flag = 0
            }

            if (flag === 1) {
                seatArray[i] = 'Available_seats'
            } else {
                seatArray[i] = 'Empty_seats'
            }
        }

        $.ajax({
            url: '/theater-management/save-seat-structure/',
            method: 'post',
            data: {
                screenId: screenId,
                seatStructure: JSON.stringify(seatArray)
            },
            dataType: 'json',
            success: (response) => {
                location.reload()
            }
        })
    }
</script>
<script>
    var availableSeats = document.getElementsByClassName('availableSeats')
    availableSeats = [].slice.call(availableSeats);
    for (var i = 0; i < availableSeats.length; i++) {
        var seats1 = document.getElementsByClassName('availableSeats')[i]
        if (seats1.value === 'Available_seats') {
            document.getElementsByClassName('seat')[i].style.background = 'rgb(49, 49, 49)'
        } else {
            document.getElementsByClassName('seat')[i].style.background = 'red'

        }

    }
</script>
