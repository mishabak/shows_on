<section class="main-section" style="margin-top: 90px;  ">
    <div style="position: relative; ">
        <div class="coverimage-size12" style="  opacity: 1;   position: absolute; box-shadow: 0px 0px 7px 2px black;">
            <img class="coverimage-size12" style="position: absolute;"
                src="/movieImages/{{singleMovieDetails._id}}Cover_photo.jpg" alt="" />
            <div class="coverimage-size12" style=" background: #000000a8; position: absolute;"></div>
        </div>
        <div style="position: absolute; display: flex; flex-direction: row;">
            <a class="popup-with-zoom-anim play-view1"
                style="margin: 45px;  width: 260px; position: relative; display: flex; align-items: center; justify-content: center;"
                href="#small-dialog{{singleMovieDetails._id}}">
                <div
                    style=" box-shadow:0px 0px 3px 0.3px  #d11e30; position: absolute; padding-top: 7px;padding-bottom: 7px;padding-left: 30px;padding-right: 30px; border-radius: 5px; background-color: #d11e30">
                    <i class="fa fa-play" style=" font-size: 30px; color: white;"></i>
                </div>

                <img style="width: 260px; " src="/movieImages/{{singleMovieDetails._id}}Poster_photo.jpg" alt="" />
            </a>
            <div style=" max-width: 750px;
  										margin: 40px auto;
  										position: relative;" id="small-dialog{{singleMovieDetails._id}}" class="zoom-anim-dialog mfp-hide">
                <iframe style="width: 100%;
                                     	 height: 400px;
                                     	 display: block;
                                     	 border-radius: 10px;
                                     	 " src="{{singleMovieDetails.Teaser}}" allow="autoplay; fullscreen"
                    allowfullscreen=""></iframe>
            </div>
            <div style="color: white; font-family:monospace;  margin-top: auto; ">
                <h1 class="bottom-space-add1">{{singleMovieDetails.Movie}}</h1>
                <h2 class="bottom-space-add1">{{singleMovieDetails.Language}} {{singleMovieDetails.Type}} movie</h2>
                <div class="box-content bottom-space-add1">
                    <h4 class="bottom-space-add1">
                        <span class="post "><span class="fa fa-clock-o"> </span>
                            {{singleMovieDetails.Hours}} Hour</span>
                    </h4>
                </div>
                {{#if SingleViewReleasedMovie}}
                <a href="/booking-movie/{{singleMovieDetails._id}}"> <button class="btn btn-danger">Book
                        now</button></a>

                {{else}}
                <h3 class="text-danger">Coming soon...</h3>
                {{/if}}

            </div>
        </div>
    </div>
</section>


